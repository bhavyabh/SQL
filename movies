--Retrieve all movies
select * from movies;

--Retrieve total number of movies
select count(*) from movies;

--find movies realsed in a specific year
select * from movies where year = 2022;

--Show all columns for Movies who have one of the following rank: 1,5,20,108,200
select * from movies where rank in (1,5,20,108,200);

--Show title of the movies that starts and ends with the letter 's'
select title from movies where title like 's%s';

--Show unique release years from movies and order them by descending.
select distinct year from movies order by year desc;

--Show the rank, title, worldwideGross of the movie with the highest worldwideGross
select rank, title, worldwideGross from movies where worldwideGross = (select max(worldwideGross) from movies);


--Label the rankings by percentiles
SELECT title, rank,
CASE 
    WHEN rank <= 50 THEN 'Top50Movies'
    WHEN rank <= 100 THEN 'Top100Movies'
    WHEN rank <= 150  THEN 'TOP150Movies'
	WHEN rank <= 200  THEN 'TOP200Movies'
    ELSE 'Bottom25Movie' 
END as groupByRank
FROM movies; 
